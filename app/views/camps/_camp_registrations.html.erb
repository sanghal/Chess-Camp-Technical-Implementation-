 <div class="panel clearfix">
<h5><%= "Registrations (#{@camp.registrations.count})"%></h5>
<h6>Student name and rating</h6>
<ul class="side-nav">
    <% @camp.students.each do |student| %>
          <%= link_to "#{student.last_name}, #{student.first_name}", student_path(student) %>
        <li> Rating: <%= student.rating.zero? ? "Unrated" : student.rating %></li>
        <!-- see note in _task_details for comments on raw() -->
    <% end %>
  </ul>
</div>
